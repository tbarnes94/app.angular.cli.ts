{
  "version": "0.0.0",
  "name": "app.angular.cli.ts",
  "license": "MIT",
  "scripts": {

    "ng": "ng",
    "typedoc": "typedoc",

    "clean:dist": "rm -rf \"dist\"",
    "clean:docs": "rm -rf \"docs\"",
    "clean:coverage": "rm -rf \"coverage\"",

    "start": "yarn run serve:proxy",
    "serve": "ng serve --open",
    "serve:proxy": "ng serve --proxy-config \"proxy.conf.json\" --open",

    "predoc": "yarn run clean:docs",
    "doc": "typedoc --options \"tsconfig.json\" --exclude \"**/*.(spec|d).ts\" \"src\"",

    "prelint": "",
    "lint": "ng lint",

    "test": "yarn run test:single",
    "test:single": "ng test --single-run",
    "pretest:coverage": "yarn run clean:coverage",
    "test:coverage": "ng test --code-coverage",
    "pretest:coverage:single": "yarn run clean:coverage",
    "test:coverage:single": "ng test --code-coverage --single-run",
    "coverage": "ng help",

    "pree2e": "yarn run clean:coverage",
    "e2e": "ng e2e",

    "prebuild": "yarn run clean:dist",
    "build": "ng build --target \"production\"",

    "preversion": "yarn run doc && yarn run lint && yarn run test:single",
    "version": "yarn run build"

  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.0",
    "@angular/cdk": "5.0.0-rc0",
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/flex-layout": "2.0.0-beta.10",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/material": "5.0.0-rc0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@angular/router": "5.0.0",
    "@ngrx/effects": "4.1.1",
    "@ngrx/router-store": "4.1.1",
    "@ngrx/store": "4.1.1",
    "@ngrx/store-devtools": "4.1.1",
    "@ngx-translate/core": "8.0.0",
    "@ngx-translate/http-loader": "2.0.0",
    "@swimlane/ngx-datatable": "11.0.3",
    "angular2-text-mask": "8.0.4",
    "core-js": "2.5.1",
    "hammerjs": "2.0.8",
    "lodash": "4.17.4",
    "rxjs": "5.5.2",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/language-service": "5.0.0",
    "@kuwas/app.angular.ts": "terrandominion/app.angular.ts#5.x.x",
    "@types/jasmine": "2.6.3",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.0.19",
    "codelyzer": "4.0.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.5",
    "protractor": "5.2.0",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typedoc": "0.9.0",
    "typescript": "2.5.3"
  }
}
