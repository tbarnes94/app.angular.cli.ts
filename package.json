{
  "version": "0.0.0",
  "name": "app.angular.cli.ts",
  "license": "MIT",
  "scripts": {

    "ng": "ng",
    "typedoc": "typedoc",

    "clean:dist": "rm -rf \"dist\"",
    "clean:docs": "rm -rf \"docs\"",
    "clean:coverage": "rm -rf \"coverage\"",

    "start": "yarn run serve:proxy",
    "serve": "ng serve --open",
    "serve:proxy": "ng serve --proxy-config \"proxy.conf.json\" --open",

    "predoc": "yarn run clean:docs",
    "doc": "typedoc --options \"typedoc.json\" --exclude \"**/*.(spec|d).ts\" \"src\"",

    "prelint": "",
    "lint": "ng lint",

    "test": "yarn run test:single",
    "test:single": "ng test --single-run",
    "pretest:coverage": "yarn run clean:coverage",
    "test:coverage": "ng test --code-coverage",
    "pretest:coverage:single": "yarn run clean:coverage",
    "test:coverage:single": "ng test --code-coverage --single-run",
    "coverage": "ng help",

    "pree2e": "yarn run clean:coverage",
    "e2e": "ng e2e",

    "prebuild": "yarn run clean:dist",
    "build": "ng build --target \"production\"",

    "preversion": "yarn run docs && yarn run lint && yarn run test:single",
    "version": "yarn run build"

  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.3.1",
    "@angular/common": "4.3.1",
    "@angular/compiler": "4.3.1",
    "@angular/core": "4.3.1",
    "@angular/flex-layout": "2.0.0-beta.7",
    "@angular/forms": "4.3.1",
    "@angular/http": "4.3.1",
    "@angular/platform-browser": "4.3.1",
    "@angular/platform-browser-dynamic": "4.3.1",
    "@angular/router": "4.3.1",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.30",
    "@ng2-dynamic-forms/core": "1.4.23",
    "@ng2-dynamic-forms/ui-bootstrap": "1.4.23",
    "@ng2-dynamic-forms/ui-ng-bootstrap": "1.4.23",
    "@ngrx/effects": "4.0.1",
    "@ngrx/router-store": "4.0.0",
    "@ngrx/store": "4.0.0",
    "@ngrx/store-devtools": "4.0.0",
    "@ngx-translate/core": "7.1.0",
    "@ngx-translate/http-loader": "0.1.0",
    "@swimlane/ngx-datatable": "10.0.5",
    "angular2-text-mask": "8.0.3",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "lodash" : "4.17.4",
    "rxjs": "5.4.2",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.0",
    "@angular/compiler-cli": "4.3.1",
    "@angular/language-service": "4.3.1",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/node": "8.0.20",
    "codelyzer": "3.1.2",
    "jasmine-core": "2.7.0",
    "jasmine-spec-reporter": "4.2.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.3",
    "protractor": "5.1.2",
    "ts-node": "3.3.0",
    "tslint": "5.5.0",
    "typedoc": "0.8.0",
    "typescript": "2.4.2"
  }
}
